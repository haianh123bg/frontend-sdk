    "@tiptap/pm": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.5",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.555.0",
    "monaco-editor": "^0.52.0",
    "react-hook-form": "^7.49.2",
    "react-hotkeys-hook": "^5.2.1",
    "react-resizable-panels": "^2.1.7",
    "reactflow": "^11.10.0",
